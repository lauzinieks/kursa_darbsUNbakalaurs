{
    "manifest_version": 3,
    "name": "Dialogflow Extension",
    "version": "1.0",
    "description": "Displays Dialogflow pop-out on https://www.lu.lv/",
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.lu.lv/*"],
        "js": ["content.js"]
      }
    ]
  }